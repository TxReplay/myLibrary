<ul id="movieList" class="list-unstyled list-inline text-center">
		{% for movie in movies.results %}
			{% set imgtmdb = 'http://image.tmdb.org/t/p/original' %}
			{% set poster = imgtmdb ~ movie.poster_path %}
			{% if movie.poster_path is null %}
				{% set poster = 'http://www.ovrtur.com/reference/img/content/no_poster.png' %}
			{% endif %}
			<li>
				<a href="{{ path('movie', { 'id': movie.id}) }}">
					<img src="{{ poster }}" alt="" width="150" height="225" />
				</a>
			</li>
		{% endfor %}
	</ul>